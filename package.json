{
    "name": "discord.js-multilingual-utils",
    "version": "1.7.3",
    "author": "Kevin Novak & Scott Bucher",
    "description": "Multilingual utilities for discord.js.",
    "license": "MIT",
    "private": false,
    "engines": {
        "node": ">=12.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "tsc --project tsconfig.json",
        "build:e2e": "tsc --project tests/e2e/tsconfig.json",
        "prepare": "rm -rf dist && npm run build",
        "test": "npm run test:unit",
        "test:unit": "ts-mocha tests/unit/**/*.test.ts",
        "test:e2e": "npm run build:e2e && node --enable-source-maps tests/e2e/dist/tests/e2e/src/app.js"
    },
    "peerDependencies": {
        "discord.js": ">=13.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.22",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.10.3",
        "chai": "^4.3.4",
        "mocha": "^8.4.0",
        "prettier": "^2.4.1",
        "ts-mocha": "^8.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.4.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KevinNovak/discord.js-Multilingual-Utils.git"
    },
    "keywords": [
        "discord",
        "discord.js",
        "embed",
        "extension",
        "extensions",
        "language",
        "localization",
        "message",
        "multilanguage",
        "multilingual",
        "util",
        "utilities",
        "utility"
    ]
}
