{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "preLaunchTask": "tsc",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "program": "${workspaceFolder}\\dist\\test.js",
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": ["<node_internals>/**"],
            "restart": true
        }
    ]
}
